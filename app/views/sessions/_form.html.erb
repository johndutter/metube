<%= form_for(:login, :url => {:controller => 'sessions', :action => 'try_login'}) do |f| %>
	<h1> Log In </h1>
  <div class="field">
    <%= f.label :username %><br>
    <%= f.text_field :username %>
  </div>
  <div class="field">
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <div class="actions">
    <%= f.submit "Log In" %>
  </div>
<% end %>
