<div class='container'>

  <div class='navbar-header'>
    <a class='navbar-brand red' href='#'>MeTube</a>
  </div>

  <form class='form-inline navbar-form navbar-left' role='search'>
    <div class='form-group search-box'>
      <input type='text' class='form-control' placeholder='Search'>
    </div>
    <button type='submit' class='btn btn-default'>Submit</button>
  </form>



  <div class='navbar-header navbar-right' ng-hide='userdata.loggedin'>
    <button type='button' class='btn btn-primary navbar-btn navbar-right' ui-sref='signup'>Sign Up</button>
    <p class='navbar-text navbar-right'>or</p>
    <button type='button' class='btn btn-primary navbar-btn navbar-right' ui-sref='login'>Log In</button>
  </div>

  <div class='navbar-header navbar-right' ng-show='userdata.loggedin'>
    <button type='button' class='btn btn-primary navbar-btn navbar-right' ng-click='logout()'>Logout</button>
  </div>

  <div class='btn-group navbar-header navbar-left upload-area' ng-show='userdata.loggedin'>
    <button type='button' class='btn btn-default navbar-btn' ui-sref='upload'><span class='glyphicon glyphicon-open'></span>Upload</button>
    <button type='button' class='btn btn-default navbar-btn dropdown-toggle' data-toggle='dropdown'>
      <span class='caret'></span>
      <span class='sr-only'>Toggle Dropdown</span>
    </button>
    <ul class="dropdown-menu" role="menu">
      <li><a ui-sref='home.popular'><span class='glyphicon glyphicon-home'></span>Home</a></li>
      <li><a ui-sref='dashboard.home'><span class='glyphicon glyphicon-th-list'></span>Dashboard</a></li>
    </ul>
  </div>

</div>