<div class='panel panel-default recommended'>
  <div class='panel-heading'>
    <h3 class='panel-title'>Recommended</h3>
  </div>
  <div class='panel-body'>

    <div class='list-entry clearfix' ng-repeat='mult in recommended'>
      <a class='pull-left' ui-sref='multimedia.sidebar({id: mult.id})'>
        <img ng-src='{{mult.thumbnail_path}}' width='100' height='60'>
      </a>
      <div class='text-area pull-left'>
        <a ui-sref='multimedia.sidebar({id: mult.id})'>{{mult.title}}</a><br>
        <span class='userarea'>by <span class='username'>{{mult.username}}</span></span>
      </div>
    </div>

  </div>
</div>