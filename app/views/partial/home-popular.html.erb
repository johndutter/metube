<div class="panel panel-default">
  <div class="panel-body">
    <h1 class='main-title'>Popular on MeTube</h1>
  </div>
</div>

<div class="panel panel-default" ng-repeat='category in categories'>
  <div class="panel-body">
    <h2>Popular in {{category.name}}</h2>
    <div class='mult-area clearfix'>
      <span class='chevron glyphicon glyphicon-chevron-left pull-left' ng-click='move("left", $index)' ng-class='{chevdisabled: category.paging.offset==0}'></span>
      <div class='row'>
        <div ng-repeat='file in category.multimedia' class='col-4'>
          <a ui-sref='multimedia.sidebar({id: file.id})' class='thumbnail'>
            <img ng-src='{{file.thumbnail_path}}' width='200' height='120' alt='{{file.title}}'></img>
            {{file.title}}
          </a>
        </div>
      </div>
      <span class='chevron glyphicon glyphicon-chevron-right pull-right' ng-click='move("right", $index);'></span>
    </div>
  </div>
</div>